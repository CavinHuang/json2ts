{
  "name": "@cyly/json2ts",
  "version": "0.0.1-beta.2",
  "description": "transform json to ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "vitest -u",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "nr build"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "author": "chenpeng cp786156072@163.com",
  "homepage": "https://github.com/ChpShy/json2ts#readme",
  "bugs": {
    "url": "https://github.com/ChpShy/json2ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChpShy/json2ts.git"
  },
  "license": "ISC",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@vitest/ui": "^0.7.4",
    "bumpp": "^7.1.1",
    "c8": "^7.11.0",
    "esbuild": "^0.14.27",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "typescript": "^4.1",
    "vite": "^2.8.6",
    "vitest": "^0.7.4"
  }
}
